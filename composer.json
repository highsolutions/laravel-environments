{
    "name": "highsolutions/laravel-environments",
    "description": "A Laravel package for easy management of different environments (dev, staging, production, etc.).",
    "keywords": ["laravel", "environments", "env", "server", "management"],
    "authors": [
        {
            "name": "HighSolutions",
            "email": "adam@highsolutions.pl"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "illuminate/console": "~5.6.0",
        "illuminate/support": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench" : "~3.6.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "HighSolutions\\LaravelEnvironments\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HighSolutions\\LaravelEnvironments\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "component": "package",
        "frameworks": ["Laravel 5.6"],
        "laravel": {
            "providers": [
                "HighSolutions\\LaravelEnvironments\\EnvironmentServiceProvider"
           ]
        }
    },
    "minimum-stability": "stable"
}
