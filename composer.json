{
    "name": "highsolutions/laravel-environments",
    "description": "A Laravel package for easy management of different environments (dev, staging, production, etc.).",
    "keywords": ["laravel", "environments", "env", "server", "management"],
    "authors": [
        {
            "name": "HighSolutions",
            "email": "adam@highsolutions.pl"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "illuminate/console": "5.*",
        "illuminate/support": "5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "6.*|7.*",
        "orchestra/testbench" : "3.*",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "HighSolutions\\LaravelEnvironments\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HighSolutions\\LaravelEnvironments\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "component": "package",
        "frameworks": [
            "Laravel 5.0",
            "Laravel 5.1",
            "Laravel 5.2",
            "Laravel 5.3",
            "Laravel 5.4",
            "Laravel 5.5",
            "Laravel 5.6"
        ],
        "laravel": {
            "providers": [
                "HighSolutions\\LaravelEnvironments\\EnvironmentServiceProvider"
           ]
        }
    },
    "minimum-stability": "stable"
}
